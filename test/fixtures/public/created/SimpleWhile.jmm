import io;
class SimpleWhile {
	public boolean compare(int a, int b) {
		return a < b;
	}

    public static void main(String[]args){
    	int n;
		SimpleWhile simpleWhile;
    	n = 1;
		simpleWhile = new SimpleWhile();

    	while (simpleWhile.compare(n, 10)) {
    		n = n + 1;
    	}
    	io.println(n);
    }
}